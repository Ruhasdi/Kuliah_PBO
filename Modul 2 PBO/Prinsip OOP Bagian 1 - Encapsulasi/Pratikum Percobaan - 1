class Hero:
    jumlah = 0
    __privateJumlah = 0

    def __init__(self, name, health):
        self.name = name
        self.health = health

        self.__private = 'private'
        self.__protected = 'protected'  # Perbaikan: "selt" diganti menjadi "self"

hero_1 = Hero('Atsu', 100)

print(hero_1.__dict__)
print(Hero.__dict__)
print(Hero.__privateJumlah)  # Perbaikan: Akses atribut privat menggunakan name mangling (_Hero__privateJumlah)